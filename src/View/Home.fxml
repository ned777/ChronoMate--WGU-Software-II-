<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/17"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Controller.HomeController"
            prefWidth="400" prefHeight="300">
    <children>
        <Label text="Welcome!" layoutX="155" layoutY="30" style="-fx-font-size: 24px;"/>
        <Button fx:id="appointmentBtn" text="Appointment" layoutX="125" layoutY="90" prefWidth="150" prefHeight="40" onAction="#goToAppointments"/>
        <Button fx:id="customerBtn" text="Customers" layoutX="125" layoutY="140" prefWidth="150" prefHeight="40" onAction="#goToCustomers"/>
        <Button fx:id="reportBtn" text="Reports" layoutX="125" layoutY="190" prefWidth="150" prefHeight="40" onAction="#goToReports"/>
    </children>
</AnchorPane>
